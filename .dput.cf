[DEFAULT]
default_host_main = notspecified

[notspecified]
fqdn = SPECIFY.A.PPA.NAME
incoming = .

[my-ppa]
fqdn = ppa.launchpad.net
method = ftp
incoming = ~dominik-stadler/ppa/ubuntu/
login = dstadler
allow_unsigned_uploads = 0

